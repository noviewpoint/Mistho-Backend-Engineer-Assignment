{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "monorepo": true,
  "root": "apps/api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "scraper": {
      "type": "application",
      "root": "apps/scraper",
      "entryFile": "main",
      "sourceRoot": "apps/scraper/src",
      "compilerOptions": {
        "tsConfigPath": "apps/scraper/tsconfig.app.json"
      }
    },
    "message-broker-client": {
      "type": "library",
      "root": "libs/message-broker-client",
      "entryFile": "index",
      "sourceRoot": "libs/message-broker-client/src",
      "compilerOptions": {
        "tsConfigPath": "libs/message-broker-client/tsconfig.lib.json"
      }
    },
    "employee": {
      "type": "library",
      "root": "libs/employee",
      "entryFile": "index",
      "sourceRoot": "libs/employee/src",
      "compilerOptions": {
        "tsConfigPath": "libs/employee/tsconfig.lib.json"
      }
    }
  }
}